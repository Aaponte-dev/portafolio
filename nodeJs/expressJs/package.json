{
  "name": "product-store-api",
  "version": "1.0.0",
  "description": "This project presents an API Restful with functionalities for an online product store",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --env-file .env dist/src/app.js",
    "build": "rm -rf ./dist && tsc",
    "dev": "npx nodemon --inspect src/app.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\" ",
    "lint:fix": "eslint \"*/**/*.{js,ts,tsx}\" --fix",
    "typeorm-migration:create": "typeorm-ts-node-commonjs migration:create --dataSource ./dataSource.ts --pretty true",
    "typeorm-migration:show": "typeorm-ts-node-commonjs migration:show --dataSource ./dataSource.ts",
    "typeorm-migration:generate": "typeorm-ts-node-commonjs migration:generate --dataSource ./dataSource.ts --pretty true ./src/modules/typeOrm/migrations/model",
    "typeorm-migration:run": "typeorm-ts-node-commonjs migration:run --dataSource ./dataSource.ts",
    "typeorm-migration:revert": "typeorm-ts-node-commonjs migration:revert --dataSource ./dataSource.ts",
    "typeorm-seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create --name",
    "typeorm-seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run --dataSource ./dataSource.ts"
  },
  "keywords": [
    "javaScript",
    "nodeJs",
    "expressJs",
    "typeScript",
    "mongoose",
    "typeorm",
    "jsonwebtoken",
    "passport",
    "swagger",
    "eslint"
  ],
  "author": "Angel Aponte <aponte_14leonardo@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@typegoose/typegoose": "^12.2.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.19",
    "typeorm-extension": "^3.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.25",
    "@types/nodemon": "^1.19.6",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.4"
  }
}